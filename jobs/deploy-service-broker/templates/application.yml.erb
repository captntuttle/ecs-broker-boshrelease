broker:
  management-endpoint: <%= properties.management_endpoint %>
  replication-group: <%= properties.replication_group %>
  namespace: <%= properties.namespace %>
  username: <%= properties.api_username%>
  password: <%= properties.api_password%>
  broker-api-version: <%= properties.api_version %>
  <% if properties.object_endpoint -%>
  object-endpoint: <%= properties.object_endpoint %>
  <% end -%>
  <% if properties.repository_endpoint -%>
  repository-endpoint: <%= properties.repository_endpoint%>
  <% end -%>
  <% if properties.certificate -%>
  certificate: ecs_cert.pem
  <% end -%>
  <% if properties.base_url -%>
  base-url: <%= properties.base_url %>
  <% end -%>
  <% if properties.repository_user -%>
  repository-user: <%= properties.repository_user%>
  <% end -%>
  <% if properties.repository_bucket -%>
  repository-bucket: <%= properties.repository_bucket %>
  <% end -%>
  <% if properties.prefix -%>
  prefix: <%= properties.prefix %>
  <% end -%>
  repository-service-id: f3cbab6a-5172-4ff1-a5c7-72990f0ce2aa
  repository-plan-id: 89d20694-9ab0-4a98-bc6a-868d6d4ecf31
security:
  <%= properties.security_username %>:
    password: <%= properties.security_password %>
catalog:
  services:
    - id: 09cac1c6-1b0a-11e6-b6ba-3e1d05defe78
      name: ecs-namespace
      description: Elastic Cloud Namespace
      bindable: true
      plan-updatable: true
      tags:
        - s3
        - namespace
      service-settings:
        service-type: namespace
      metadata:
        displayName: ecs-namespace
        imageUrl: http://www.emc.com/images/products/header-image-icon-ecs.png
        longDescription: EMC Elastic Cloud Storage (ECS) Namespace for managing Amazon S3, Swift or Atmos buckets.
        providerDisplayName: EMC Corporation
        documentationUrl: https://community.emc.com/docs/DOC-45012
        supportUrl: http://www.emc.com/products-solutions/trial-software-download/ecs.htm
      plans:
        - id: 09cac5b8-1b0a-11e6-b6ba-3e1d05defe78
          name: 5gb
          description: Free Trial
          service-settings:
            quota:
              limit: 5
              warn: 4
          metadata:
            costs:
              - amount:
                  usd: 5.0
                unit: MONTHLY
            bullets:
              - Multiple Bucket Namespace
              - 5 GB Storage
              - S3 protocol and HDFS access
    - id: f3cbab6a-5172-4ff1-a5c7-72990f0ce2aa
      name: ecs-bucket
      type: bucket
      description: Elastic Cloud S3 Object Storage Bucket
      bindable: true
      plan-updatable: true
      tags:
        - s3
        - bucket
      service-settings:
        service-type: bucket
        head-type: s3
        file-system-enabled: false
        access-during-outage: true
      metadata:
        displayName: ecs-bucket
        imageUrl: http://www.emc.com/images/products/header-image-icon-ecs.png
        longDescription: EMC Elastic Cloud Storage (ECS) Object bucket for storing data using Amazon S3, Swift or Atmos protocols.
        providerDisplayName: EMC Corporation
        documentationUrl: https://community.emc.com/docs/DOC-45012
        supportUrl: http://www.emc.com/products-solutions/trial-software-download/ecs.htm
      plans:
        - id: 8e777d49-0a78-4cf4-810a-b5f5173b019d
          name: 5gb
          description: Free Trial
          service-settings:
            quota:
              limit: 5
              warn: 4
          free: true
          metadata:
            costs:
              - amount:
                  usd: 0.0
                unit: MONTHLY
            bullets:
              - Shared object storage
              - 5 GB Storage
              - S3 protocol and HDFS access
        - id: 89d20694-9ab0-4a98-bc6a-868d6d4ecf31
          name: unlimited
          description: Pay per GB for Month
          metadata:
            costs:
              - amount:
                  usd: 0.03
                unit: PER GB PER MONTH
            bullets:
              - Shared object storage
              - Unlimited Storage
              - S3 protocol and HDFS access
